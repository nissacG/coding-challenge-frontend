{
  "extends": ["wesbos"],
  "rules": {
    "no-eval": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "jsx-quotes": ["error", "prefer-single"],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 80,
        "semi": false,
        "jsxSingleQuote": true
      }
    ]
  }
}
